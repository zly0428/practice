import java.util.Scanner;

public class Solution {
	
	public static void main(String args[]) {
		
		System.out.println(isPalindrome(121));
	}
	public static boolean isPalindrome(int x) {
		int t=0;
		while (x !=0)
		{
			
			t=(t+(x%10))*10;
			x=x/10;
		}
		if (x ==(t/10))
		{
			return true;
		}
		else{
			return false;
		}
	}

}

